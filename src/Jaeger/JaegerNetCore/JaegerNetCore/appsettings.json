{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "Jaeger.Reporters.LoggingReporter": "Information",
        "Jaeger.Example.WebApi.TracingMiddleware": "Information"
      }
    },
    "LoggingReporter": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    //redis 服务器地址
    //"RedisService": "192.168.3.92:6379,ssl=false,writeBuffer=1024000",
    "RedisService": "192.168.3.10:6801,ssl=false,writeBuffer=10240",
    // 用户权限验证过期时间 单位（秒）
    "UserActionTimeOut": 600
  }
}
